{
  "name": "clickalicious/cachingmiddleware",
  "description": "CachingMiddleware - The PSR-6 Caching Middleware compatible to PSR-7 stacks.",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Lead"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.0",
    "shrikeh/teapot": "^1.0",
    "psr/http-message": "^1.0",
    "gpupo/cache": "^1.2",
    "wandu/http": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "relay/relay": "^1.1",
    "symfony/var-dumper": "^2.8",
    "cocur/slugify": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Clickalicious\\": "src/Clickalicious/"
    }
  },
  "include-path": [
    "src/"
  ],
  "keywords": [
    "psr-7",
    "middleware",
    "cache",
    "caching",
    "relay",
    "stack",
    "psr-6"
  ],
  "homepage": "https://github.com/clickalicious/CachingMiddleware",
  "license": "BSD-3-Clause",
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/CachingMiddleware/issues",
    "wiki": "https://github.com/clickalicious/CachingMiddleware/wiki",
    "tests": "https://clickalicious.github.io/CachingMiddleware"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  }
}
